{
  "name": "scriptbox",
  "version": "0.0.1",
  "description": "A server for the sandbox programming platformer Scriptbox",
  "private": true,
  "main": "main.ts",
  "engines": {
    "node": ">=10.4.0"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --verbose false --coverage",
    "watch-build": "tsc --p \"./src\" -w",
    "watch-copy-scripts": "cpx -w \"./scripts/**/*.ts\" dist/scripts",
    "watch-copy-res": "cpx -w \"./res/**/*\" dist",
    "watch-copy-data-default": "cpx -w \"./data-default/**/*\" dist/data-default",
    "watch-test": "jest --watch",
    "watch": "npm-run-all --parallel watch-build watch-copy-scripts watch-copy-data-default && npm run watch-copy-res",
    "build": "tsc --p \"./src\" && npm run copy-scripts && npm run copy-data-default && npm run copy-res && npm run test",
    "copy-scripts": "cpx \"./scripts/**/*.ts\" dist/scripts",
    "copy-res": "cpx \"./res/**/*\" dist",
    "copy-data-default": "cpx \"./data-default/**/*\" dist/data-default",
    "start": "cd dist && node main.js",
    "start-test": "cd dist && node main-test.js"
  },
  "repository": {
    "type": "git",
    "url": "http://www.github.com/scriptbox-team/scriptbox"
  },
  "author": "Aaron Freytag",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.15",
    "@types/request": "^2.48.3",
    "@types/supertest": "^2.0.8",
    "cpx": "^1.5.0",
    "jest": "^24.8.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.6",
    "typedoc-plugin-external-module-name": "^2.1.0"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.137",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.3.3",
    "@types/ws": "^6.0.1",
    "browser-process-hrtime": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "file-type": "^12.4.0",
    "fs-extra": "^8.1.0",
    "isolated-vm": "^1.7.10",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.0",
    "mongodb": "^3.3.3",
    "public-ip": "^4.0.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.12",
    "supertest": "^4.0.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.5.3",
    "ws": "^6.2.1"
  }
}
